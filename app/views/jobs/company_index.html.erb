All Jobs for <strong><%= @company.name %></strong>

<% @jobs.each do |job| %>
	<ul><%=link_to "#{job.title}", company_job_path(@company,job) %> at <%= job.company.name %>
		<%=link_to "Edit", edit_company_job_path(@company,job)%>
		<%=link_to "Delete", company_job_path(@company,job), method: "delete"%>
	</ul>
<% end %>

<%= link_to "New Job", new_company_job_path(@company) %>
<h4>Company Contact:</h4>

<ul>
	<li>Name: <%= @contact.name %></li>
	<li>Position: <%= @contact.position%></li>
	<li>Email: <%= @contact.email %></li>
</ul>

<h3>Back to <%=link_to 'Companies', '/companies'%></h3>

